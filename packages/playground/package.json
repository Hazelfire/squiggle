    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.16",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "ace-builds": "^1.4.12",
    "antd": "^4.18.5",
    "autoprefixer": "9.8.8",
