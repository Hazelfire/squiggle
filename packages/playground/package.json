    "bs-css": "^15.1.0",
    "bs-css-emotion": "^4.1.0",
    "bs-moment": "0.6.0",
    "bsb-js": "1.1.7",
    "cross-env": "^7.0.3",
    "css-loader": "^6.6.0",
    "d3": "7.3.0",
    "gh-pages": "2.2.0",
    "jest": "^25.5.1",
    "jstat": "1.9.2",
    "lenses-ppx": "5.1.0",
    "less": "3.10.3",
